{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Attach",
			"program": "${workspaceFolder}/server/app.js",
			"port": 9229
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "vuejs: chrome",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}/webapp/src",
			"breakOnLoad": true,
			"sourceMapPathOverrides": {
				// "webpack:///src/components/*": "${webRoot}/components/*",
				"webpack:///src/*": "${webRoot}/*"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest All",
			"program": "${workspaceFolder}/server/node_modules/.bin/jest",
			"args": [
				"--runInBand"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"windows": {
				"program": "${workspaceFolder}/server/node_modules/jest/bin/jest"
			}
		},
		{
			"type": "node",
			"request": "launch",
			"name": "Jest Current File",
			"program": "${workspaceFolder}/server/node_modules/.bin/jest",
			"runtimeVersion": "10.11.0",
			"args": [
				"${relativeFile}"
			],
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen",
			"windows": {
				"program": "${workspaceFolder}/server/node_modules/jest/bin/jest"
			}
		}
	]
}